        self.valor = valor
        self.sala = sala

    def cadastrarCurso(self):
        pass
    def exibirCurso(self):
        pass
    def calcularNumMinAluno(self):
        pass

class Salario():
    def __init__(self, salarioBruto, salarioLiquido, inss, irrf, planoSaude):
        self.salarioBruto = salarioBruto
        self.salarioLiquido = salarioLiquido
        self.inss = inss
        self.irrf = irrf
        self.planoSaude = planoSaude

    def calcularSalario(self):
        pass

class Funcionario(Pessoa):
    def __init__(self,nome, rg, cpf, anoNasc, mesNasc, diaNasc, sexo, matricula, setor, cargo, nivel):
        super().__init__(nome, rg, cpf, anoNasc, mesNasc, diaNasc, sexo)
        self.matricula = matricula
        self.setor = setor
        self.cargo = cargo
        self.nivel = nivel

    def atribuirSalario(self):
        if self.nivel == 'A':
            salario = 1520.25
        elif self.nivel == 'B':
            salario = 2362.67
        elif self.nivel == 'C':
            salario = 2988.92
        elif self.nivel == 'D':
            salario = 3572.77
        elif self.nivel == 'E':
            salario = 4878.67


    def cadastrarFuncionario(self):
        pass
    def exibirFuncionario(self):
        pass

class CoordenadorAdmin(Funcionario):
    def __init__(self,nome, rg, cpf, anoNasc, mesNasc, diaNasc, sexo, matricula, setor, cargo, nivel, area, plusSalario):
        super().__init__(nome, rg, cpf, anoNasc, mesNasc, diaNasc, sexo, matricula, setor, cargo, nivel) #funcionario
        self.area = area
        self.plusSalario = plusSalario

    def cadastrarCoordenadorAdmin(self):
        pass
    def exibirCoordenadroAdmin(self):
        pass
    def calcularPlusSalario(self):
        pass

class Professor(Funcionario):
    def __init__(self, nome, rg, cpf, anoNasc, mesNasc, diaNasc, sexo, matricula, setor, cargo, nivel, formacao, nivel1, disciplina):
        super().__init__(nome, rg, cpf, anoNasc, mesNasc, diaNasc, sexo, matricula, setor, cargo, nivel)
        self.formacao = formacao
        self.nivel1 = nivel1
        self.disciplina = disciplina
    def atribuirSalario(self):
        if self.nivel1 == 'I':
            salario = 6500.00
        elif self.nivel1 == 'II':
            salario = 8325,50
        elif self.nivel1 == 'III':
            salario = 12568.43


    def cadastrarProfessor(self):
        pass
    def exibirProfessor(self):
        pass

class Coordenador(Professor):
